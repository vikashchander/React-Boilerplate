{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "React Boilerplate",
  "main": "index.js",
  "scripts": {
    "debug": "nodemon --inspect src/server/server.js",
    "build": "webpack  --config webpack.config.js ",
    "build-babel": "npm run build-babel-server && npm run build-babel-client",
    "build-babel-server": "babel src/server --out-dir ./dist",
    "build-babel-client": "babel src/client --copy-files --out-dir ./dist/public ",
    "start": "node ./dist/@babel/polyfill.js",
    "test": "jest ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vikashchander/mern-boilerplate.git"
  },
  "keywords": [
    "node",
    "react"
  ],
  "author": "vikash chander",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vikashchander/mern-boilerplate/issues"
  },
  "homepage": "https://github.com/vikashchander/mern-boilerplate#readme",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "express": "^4.17.1",
    "jest-svg-transformer": "^1.0.0",
    "raf": "^3.4.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rimraf": "^3.0.2"
  },
  "jest": {
    "transform": {
      "^.+\\.(js?)$": "babel-jest",
      "^.+\\.svg$": "jest-svg-transformer"
    },
    "setupFilesAfterEnv": [
      "./src/enzyme.setup.js"
    ],
    "setupFiles": [
      "raf/polyfill"
    ],
    "verbose": true
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "babel-jest": "^27.0.2",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "css-loader": "^5.2.6",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.6.0",
    "nodemon": "^2.0.7",
    "react-test-renderer": "^16.14.0",
    "source-map-loader": "^3.0.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-node-externals": "^3.0.0"
  }
}
