{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "React Boilerplate",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.config.js ",
    "build-babel": "npm run build-babel-server && npm run build-babel-client",
    "build-babel-server": "babel src/server --out-dir ./dist",
    "build-babel-client": "babel src/client --copy-files --out-dir ./dist/public",
    "start": "node ./dist/server.js",
    "test": "jest ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vikashchander/mern-boilerplate.git"
  },
  "keywords": [
    "node",
    "react"
  ],
  "author": "vikash chander",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vikashchander/mern-boilerplate/issues"
  },
  "homepage": "https://github.com/vikashchander/mern-boilerplate#readme",
  "dependencies": {
    "express": "^4.17.1",
    "raf": "^3.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./src/enzyme.setup.js",
    "setupFiles": [
      "raf/polyfill"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^26.6.3",
    "react-test-renderer": "^17.0.2",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0",
    "webpack-node-externals": "^3.0.0"
  }
}
